<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - IntexFinal</title>
    <link rel="stylesheet" href="~/lib/items/bootstrap/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <!-- favicon -->
    <link rel="icon" type="image/x-icon" href="/img/car2-removebg-preview.png">
    <!-- google font -->
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,700" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Poppins:400,700&display=swap" rel="stylesheet">
    <!-- fontawesome -->
    <!--<link rel="stylesheet" href="~/lib/items/bootstrap/css/all.min.css">-->
    <!-- bootstrap -->
    <link rel="stylesheet" href="~/lib/items/bootstrap/css/bootstrap.min.css">
    <!-- owl carousel -->
    <link rel="stylesheet" href="~/lib/items/css/owl.carousel.css">
    <!-- magnific popup -->
    <link rel="stylesheet" href="~/lib/items/css/magnific-popup.css">
    <!-- animate css -->
    <link rel="stylesheet" href="~/lib/items/css/animate.css">
    <!-- mean menu css -->
    <link rel="stylesheet" href="~/lib/items/css/meanmenu.min.css">
    <!-- main style -->
    <link rel="stylesheet" href="~/lib/items/css/main.css">
    <!-- responsive -->
    <link rel="stylesheet" href="~/lib/items/css/responsive.css">
</head>
<body>
    <!-- header -->
    <div class="top-header-area" id="sticker">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 col-sm-12 text-center">
                    <div class="main-menu-wrap">
                        <!-- logo -->
                        <div class="site-logo">
                            <a asp-controller="Home" asp-action="Index">
                                <img src="/img/car2-removebg-preview.png" alt="" height="30">
                            </a>
                        </div>
                        <!-- logo -->
                        <!-- menu start -->
                        <nav class="main-menu">
                            <ul>
                                <li><a asp-controller="Home" asp-action="Index">Home</a></li>
                                <li><a asp-controller="Home" asp-action="Incidents">Incidents</a></li>
                                <li><a asp-controller="Home" asp-action="SeverityCalculator">Severity Calculator</a></li>
                            </ul>
                        </nav>
                        <!-- menu end -->
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="bg-dark" style="height:100px;"></div>
    <partial name="_CookieConsentPartial" />
    <!-- end header -->
    @RenderBody()
    <!-- footer -->
    <!-- end footer -->
    <!-- copyright -->
    <div class="copyright">
        <div class="container">
            <div class="row">
                <div class="col-lg-6 col-md-12">
                    <p>
                        Yeet &copy; 2022 - Group 2.5 - <a asp-controller="Home" asp-action="Privacy">Privacy</a>
                    </p>
                </div>
                <div class="col-lg-6 text-right col-md-12">

                </div>
            </div>
        </div>
    </div>
    <script src="https://unpkg.com/feather-icons"></script>
    <script>
        feather.replace()
    </script>
    <script src="~/lib/items/js/jquery-1.11.3.min.js"></script>
    <!-- bootstrap -->
    <script src="~/lib/items/bootstrap/js/bootstrap.min.js"></script>
    <!-- count down -->
    <script src="~/lib/items/js/jquery.countdown.js"></script>
    <!-- isotope -->
    <script src="~/lib/items/js/jquery.isotope-3.0.6.min.js"></script>
    <!-- waypoints -->
    <script src="~/lib/items/js/waypoints.js"></script>
    <!-- owl carousel -->
    <script src="~/lib/items/js/owl.carousel.min.js"></script>
    <!-- magnific popup -->
    <script src="~/lib/items/js/jquery.magnific-popup.min.js"></script>
    <!-- mean menu -->
    <script src="~/lib/items/js/jquery.meanmenu.min.js"></script>
    <!-- sticker js -->
    <script src="~/lib/items/js/sticker.js"></script>
    <!-- main js -->
    <script src="~/lib/items/js/main.js"></script>
    <!-- end copyright -->
    <!-- jquery -->
    <!--PreLoader-->
    <div class="loader">
        <div class="loader-inner">
            <div class="circle"></div>
        </div>
    </div>
    <!--PreLoader Ends-->
</body>
</html>

@RenderSection("scripts", required: false)